{
  "name": "pixline/wp-cli-php-devtools",
  "type": "command",
  "description": "Wrapper around common development utilities",
  "keywords": ["wp-cli","phpcpd","phpcs","phpdcd","phploc","phpmd","phpunit","phpreport","development"],
  "homepage": "https://github.com/pixline/wp-cli-php-devtools",
  "license": "MIT",
  "authors": [
    {
      "name": "Paolo Tresso",
      "email": "plugins@swergroup.com",
      "homepage": "http://swergroup.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/WordPress-Coding-Standards/WordPress-Coding-Standards/"
    }
  ],
  "require": {
    "php": ">=5.4",
    "wp-cli/wp-cli": ">=0.12",
    "phpunit/phpunit": "3.7.*",
    "phpunit/php-invoker": ">=1.1.0",
    "squizlabs/PHP_CodeSniffer": ">=1.5.1",
    "WordPress-Coding-Standards/WordPress-Coding-Standards": "master",
    "phploc/phploc": "2.0.*",
    "sebastian/phpcpd": "2.0.*",
    "phpunit/phpcov": "2.0.*@dev",
    "sebastian/phpdcd": "1.0.*@dev",
    "phpmd/phpmd": "1.5.*"
  },
  "suggest": {
    "pixline/wp-cli-proxy": "dev-master"
  },
  "support": {
    "issues": "https://github.com/pixline/wp-cli-devtools/issues",
    "source": "https://github.com/pixline/wp-cli-devtools"
  },
  "autoload": {
    "files": [ "default.php" ]
  }
}

{
  "name": "pixline/wp-cli-devtools",
  "type": "command",
  "description": "Wrapper around common development utilities",
  "keywords": ["wp-cli","minify","phpcov","phpunit","phpcpd","phpdcd","phpdoc","phploc","phpmd","phpunit","development"],
  "homepage": "https://github.com/pixline/wp-cli-devtools",
  "license": "MIT",
  "authors": [
    {
      "name": "Paolo Tresso",
      "email": "plugins@swergroup.com",
      "homepage": "http://swergroup.com",
      "role": "Developer"
    }
  ],
  "repositories": [
      {
        "type":"vcs",
        "url":"https://github.com/pixline/PHP_CodeSniffer"
      }
  ],
  "require": {
    "php": ">=5.4",
    "wp-cli/wp-cli": ">=0.12",
    "psy/psysh": "dev-master",
    "phpunit/phpunit": "3.7.*",
    "phpunit/php-invoker": ">=1.1.0",
    "pixline/php_codesniffer": "dev-master",
    "phploc/phploc": "2.0.*",
    "sebastian/phpcpd": "2.0.*",
    "phpunit/phpcov": "2.0.*@dev",
    "sebastian/phpdcd": "1.0.*@dev",
    "phpdocumentor/phpdocumentor": "v2.1.0",
    "phpmd/phpmd": "1.5.*",
    "packagist/yuicompressor-bin": "v2.4.8"
  },
  "support": {
    "issues": "https://github.com/pixline/wp-cli-devtools/issues",
    "source": "https://github.com/pixline/wp-cli-devtools"
  },
  "autoload": {
    "files": [ "default.php" ]
  }
}
